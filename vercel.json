{
  "version": 2,
  "builds": [
    {
      "src": "dist/Angular-test/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(assets|.*\\.js|.*\\.css|.*\\.json|favicon\\.ico)",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/api/index.js"
    }
  ]
}
