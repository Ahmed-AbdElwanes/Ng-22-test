{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "dist/Angular-test/server/server.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/Angular-test/server/server.mjs"
    }
  ]
}
